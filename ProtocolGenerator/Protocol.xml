<?xml version="1.0" encoding="utf-8"?>
<DOCUMENT>
  <PROTOCOLS version="1.0">
    <protocol name="AckResult" brief="디폴트 응답 프로토콜">
      <param name="result" type="uint8" brief="성공여부, 0이면 성공"/>
      <param name="msg" type="string" brief="에러 메시지"/>
    </protocol>
    <!-- 테스트 프로토콜 -->
    <protocol name="ReqTest" brief="테스트 프로토콜 타입">
      <param name="test0" type="string" brief="문자열"/>
      <param name="test1" type="float" brief="소수점 4바이트"/>
      <param name="test2" type="double" brief="소수점 8바이트"/>
      <param name="test3" type="uint8" brief="정수 1바이트"/>
      <param name="test4" type="uint16" brief="정수 2바이트"/>
      <param name="test5" type="uint32" brief="정수 4바이트"/>
      <param name="test6" type="uint64" brief="정수 8바이트"/>
    </protocol>
    <protocol name="AckTest" brief="테스트 프로토콜에 대한 응답">
      <param name="result" type="uint8" brief="성공여부, 0이면 성공"/>
      <param name="msg" type="string" brief="에러 메시지"/>
    </protocol>
  </PROTOCOLS>
</DOCUMENT>
